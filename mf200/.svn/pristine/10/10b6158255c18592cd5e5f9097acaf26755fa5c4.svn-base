e id=\"no\">————</font><span>规格：</span><font v-if=\"wuliao.m_desc\" id=\"desc\">{{wuliao.m_desc}}</font><font v-else id=\"desc\">————</font></p>\r\n            <p class=\"line-input\">数量&nbsp;&nbsp;<input type=\"text\" name=\"num\" id=\"num\" placeholder=\"请输入数量\" @input=\"change1()\"> <font id=\"unit\">{{wuliao.unit}}</font><span>单价&nbsp;&nbsp;</span><input type=\"text\" name=\"price\" id=\"price\" placeholder=\"请输入单价\" @input=\"change2()\"> 元</p>\r\n            <p class=\"line-input\">金额&nbsp;&nbsp;<font id=\"je\">————</font> 元</p>\r\n        </div>\r\n    </div>\r\n</div>\r\n</template>\r\n<script>\r\n    export default {\r\n        data() {\r\n            return {\r\n                data:[],\r\n                workers:[],\r\n                worker:[],\r\n                group:[],\r\n                supply:[],\r\n                list:[],\r\n                cate:[],\r\n                mate:[],\r\n                wuliao:[],\r\n                close_: 1000,\r\n                id: 1,\r\n                lists: []\r\n            }\r\n        },\r\n        mounted:function(){\r\n            var order_id = this.$route.query.order_id;\r\n\t\t\tthis.getlist(order_id);\r\n        },\r\n        methods: {\r\n            getlist(order_id){\r\n               \tvar sendData = {};\r\n\t\t\t\tvar jsonData = {};\r\n\t\t\t\tsendData.url = \"/index.php/pc/Order/edit\";\r\n                sendData.data = jsonData;\r\n                jsonData.order_id = order_id;\r\n\t\t\t\tvar re = getFaceInfo(sendData);\r\n\t\t\t\tif(re.status == 1){\r\n                    this.data = re.data;\r\n                    this.workers = re.workers;\r\n                    this.worker = re.worker;\r\n                    this.group = re.group;\r\n                    this.supply = re.supply;\r\n                    this.cate = re.cate;\r\n                    $('#group').val(re.data.group_id);\r\n                    $('#worker').val(re.data.worker_id);\r\n                    $('#supply').val(re.data.supply_id);