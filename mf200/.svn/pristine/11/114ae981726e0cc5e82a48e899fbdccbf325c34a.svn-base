         <option>请选择作物</option>\r\n                <option v-for=\"item in catall\" v-bind:value=\"item.cat_name\">{{ item.cat_name }}</option>\r\n              </select>\r\n\t\t\t\t\t\t</div>\r\n\r\n\t\t\t\t\t\t<div class=\"control-group\">\r\n\t\t\t\t\t\t\t<label class=\"control-label\" for=\"input01\">果型</label>\r\n\t\t\t\t\t\t\t<!-- <input type=\"text\" class=\"form-control\" style=\"width:15%;\"  name=\"username\" placeholder=\"请输入果型\" > -->\r\n              <select class=\"form-control\" style=\"width:15%; margin-left:13px;\" id=\"cat_guoxing\">\r\n\r\n                <option>请选择果型</option>\r\n                <option v-for=\"itemg in catguoxinglist\" v-bind:value=\"itemg.ft_name\">{{ itemg.ft_name }}</option>\r\n\r\n              </select>\r\n\t\t\t\t\t\t\t\t&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;\r\n\r\n\t\t\t\t\t\t\t<label class=\"control-label\" for=\"input01\">果色</label>\r\n\t\t\t\t\t\t\t<!--<input type=\"text\" class=\"form-control\" style=\"width:15%;\"  name=\"username\" placeholder=\"请输入果色\" >-->\r\n              <select class=\"form-control\" style=\"width:15%; margin-left:13px;\" id=\"cat_guose\" >\r\n\r\n                <option>请选择果色</option>\r\n                <option v-for=\"items in catguoselist\" v-bind:value=\"items.ft_name\"  >{{ items.ft_name }}</option>\r\n\r\n              </select>\r\n\r\n\t\t\t\t\t\t</div>\r\n\t\t\t\t\t\r\n\t\t\t\t\r\n\t\t\t\t\t\r\n\t\t\t\t\t\t<div class=\"control-group\">\r\n\t\t\t\t\t\t\t<label class=\"control-label\" for=\"input01\">品种</label>\r\n\t\t\t\t\t\t\t<input type=\"text\" class=\"form-control\" style=\"width:15%;\"  name=\"cat_chid_name\" placeholder=\"请输入品种\" >\r\n\t\t\t\t\t\t\t&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;\r\n\t\t\t\t\t\t\t<label class=\"control-label\" for=\"input01\">编号</label>\r\n\t\t\t\t\t\t\t<input type=\"text\" class=\"form-control\" style=\"width:15%;\"  name=\"cat_no\" placeholder=\"请输入编号\" >\r\n\t\t\t\t\t\t</div>\r\n\r\n\t\t\t\t\t\r\n\t\t\t\t\t\t<div class=\"control-group\">\r\n\t\t\t\t\t\t\t<label>描述</label>\r\n\t\t\t\t\t\t\t<textarea class=\"form-control\" name=\"cat_desc\" style=\"width:37%;\" rows=\"3\"></textarea>\r\n\t\t\t\t\t\t</div>\r\n\t\t\t\t\t\r\n\t\t\t\t\t\t<div class=\"control-group\">\r\n\t\t\t\t\t\t\t<!--<label class=\"control-label\">图片</label>\r\n\t\t\t\t\t\t\t<div class=\"col-xs-12 col-sm-4 col-md-4\" >\r\n\t\t\t\t\t\t\t\t<div class=\"file-container\" v-on:click=\"addd\" style=\"display:inline-block;position:relative;overflow: hidden;vertical-align:middle;\" >\r\n\t\t\t\t\t\t\t\t\t\r\n                  <span type=\"button\"><img src=\"/lib/img/public/cropmode/upload-button.jpg\"  ></span>\r\n\t\t\t\t\t\t\t\t\t<input type=\"file\" id=\"jobData\" onChange=\"loadFile(this.files[0])\" style=\"position:absolute;top:0;left:0;font-size:34px; opacity:0\">\r\n                  <div id=\"dndArea\" class=\"placeholder\">\r\n                        <div id=\"filePicker\" ></div>\r\n                  </div>\r\n                  \r\n\t\t\t\t\t\t\t\t</div>\r\n\t\t\t\t\t\t\t</div> \r\n              -->\r\n\t\t\t\t\t\t</div>\t\r\n\t\t\t\t\t\t</fieldset>\r\n\t\t\t\t\t</form>\r\n\t\t\t\t</div>\r\n       \r\n      \r\n\r\n\t\t\t\t<div style=\"clear:both\"></div>\r\n\t\t\t</div>\r\n\t\t\t<div style=\"clear:both\"></div>\r\n\t\t</div>\r\n\t</div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  data(){\r\n    return{\r\n      catall:[],\r\n      catguoxinglist:[],\r\n      abcd:\"\",\r\n      catguoselist:[]\r\n    }\r\n  },\r\n\r\n  mounted:function(){\r\n       this.getcat()\r\n  },\r\n\r\n  methods:{\r\n\r\n    getcat(){\r\n      //获取作物下拉框\r\n      var sendData = {};\r\n      var jsonData = {};\r\n      sendData.url =\"index.php/baseset/Crop/crop_list\";\r\n      sendData.data = jsonData;\r\n      var re = getFaceInfo(sendData);\r\n      this.catall = re.data;\r\n\r\n      /*\r\n      sendData.url =\"27.221.53.90:880/index.php/baseset/Crop/crop_list\";\r\n      jsonData.token = \"3f1tabe0jtumhnr56qkolh44m0\";\r\n      jsonData.phone = \"18114158894\";\r\n      sendData.data = jsonData;\r\n      $.ajax({\r\n        url: \"http://www.pc200.com/router.php\",\r\n        data: sendData,\r\n        dataType: \"Json\",\r\n        success: function(msg) {\r\n          this.catall = msg.data\r\n        }.bind(this),\r\n        error: function(msg) {\r\n          //alert(\"错误\");\r\n        }\r\n      });*/\r\n\r\n    },\r\n\r\n    choosecid(obj){\r\n      this.abcd=obj.value;\r\n      //console.log(obj.name);\r\n      var that=this;\r\n      //console.log(obj.value); \r\n      //this.$options.methods.catguoxing(this.abcd);\r\n      //this.$options.methods.catguose(this.abcd);\r\n      var sendData = {};\r\n      var jsonData = {};\r\n      sendData.url =\"index.php/baseset/Crop/type_list\";\r\n      jsonData.cat_id = obj.name;\r\n      sendData.data = jsonData;\r\n      var re = getFaceInfo(sendData);\r\n      if(re.status==1){\r\n            var ab=re.data;\r\n            for(var $i=0;$i<ab.length; $i++){\r\n              that.catguoxinglist.push(ab[$i]);\r\n            }\r\n      }else{\r\n        layer.msg(re.msg);\r\n      }\r\n      /*\r\n      sendData.url =\"27.221.53.90:880/index.php/baseset/Crop/type_list\";\r\n      jsonData.token = \"3f1tabe0jtumhnr56qkolh44m0\";\r\n      jsonData.phone = \"18114158894\";\r\n      jsonData.cat_id = obj.name;\r\n      sendData.data = jsonData;\r\n      $.ajax({\r\n        url: \"http://www.pc200.com/router.php\",\r\n        data: sendData,\r\n        dataType: \"Json\",\r\n        success: function(msg) {\r\n          //console.log(msg.data);          \r\n          //this.catguoxinglist = msg.data;\r\n          if(msg.status==1){\r\n            var ab=msg.data;\r\n            for(var $i=0;$i<ab.length; $i++){\r\n              that.catguoxinglist.push(ab[$i]);\r\n            }\r\n          }\r\n          \r\n        },\r\n        error: function(msg) {\r\n          //alert(\"错误\");\r\n        }\r\n      });*/\r\n\r\n      if(obj.value){\r\n          var vm=this;\r\n          var sendData = {};\r\n          var jsonData = {};\r\n          sendData.url =\"index.php/baseset/Crop/color_list\";\r\n          jsonData.cat_id = obj.value;\r\n          sendData.data = jsonData;\r\n          var re = getFaceInfo(sendData);\r\n          if(re.status==1){\r\n                 var ab=re.data;\r\n                 for(var $i=0;$i<ab.length; $i++){\r\n                    that.catguoselist.push(ab[$i]);\r\n                 }\r\n          }else{\r\n              layer.msg(re.msg);\r\n          }\r\n          /*\r\n          sendData.url =\"27.221.53.90:880/index.php/baseset/Crop/color_list\";\r\n          jsonData.token = \"3f1tabe0jtumhnr56qkolh44m0\";\r\n          jsonData.phone = \"18114158894\";\r\n          jsonData.cat_id = obj.value;\r\n          sendData.data = jsonData;\r\n          $.ajax({\r\n            url: \"http://www.pc200.com/router.php\",\r\n            data: sendData,\r\n            dataType: \"Json\",\r\n            success: function(msg) {\r\n              //alert(msg.data);\r\n              //console.log();\r\n              //this.catguoselist = msg.data\r\n              if(msg.status==1){\r\n                 var ab=msg.data;\r\n                 for(var $i=0;$i<ab.length; $i++){\r\n                    that.catguoselist.push(ab[$i]);\r\n                 }\r\n              }\r\n            },\r\n            error: function(msg) {\r\n              //alert(\"错误\");\r\n            }\r\n          });*/\r\n        }else{\r\n          alert(\"错误\");\r\n        }\r\n    },\r\n\r\n    /*\r\n    //获得果型下拉框\r\n    catguoxing(gxid){      \r\n      var sendData = {};\r\n      var jsonData = {};\r\n      sendData.url =\"27.221.53.90:880/index.php/baseset/Crop/pc_type_list\";\r\n      jsonData.token = \"3f1tabe0jtumhnr56qkolh44m0\";\r\n      jsonData.phone = \"18114158894\";\r\n      jsonData.cat_id = gxid;\r\n      sendData.data = jsonData;\r\n      $.ajax({\r\n        url: \"http://www.pc200.com/router.php\",\r\n        data: sendData,\r\n        dataType: \"Json\",\r\n        success: function(msg) {\r\n          //console.log(msg.data);          \r\n          this.catguoxinglist = msg.data;\r\n        }.bind(this),\r\n        error: function(msg) {\r\n          //alert(\"错误\");\r\n        }\r\n      });\r\n    },\r\n\r\n    //获取果色的下拉框\r\n    catguose(gsid){\r\n      var sendData = {};\r\n      var jsonData = {};\r\n      sendData.url =\"27.221.53.90:880/index.php/baseset/Crop/pc_color_list\";\r\n      jsonData.token = \"3f1tabe0jtumhnr56qkolh44m0\";\r\n      jsonData.phone = \"18114158894\";\r\n      jsonData.cat_id = gsid;\r\n      sendData.data = jsonData;\r\n      $.ajax({\r\n        url: \"http://www.pc200.com/router.php\",\r\n        data: sendData,\r\n        dataType: \"Json\",\r\n        success: function(msg) {\r\n          //alert(msg.data);\r\n          //console.log();\r\n          this.catguoselist = msg.data\r\n        }.bind(this),\r\n        error: function(msg) {\r\n          //alert(\"错误\");\r\n        }\r\n      });\r\n\r\n    },\r\n    */\r\n\r\n\r\n    catadd(){\r\n\r\n      var catname=$(\"#cat_name\").val();\r\n      var guoxing=$(\"#cat_guoxing\").val();\r\n      var guose=$(\"#cat_guose\").val();\r\n      var pinzhong=$(\"input[name=cat_chid_name]\").val();\r\n      var bianhao=$(\"input[name=cat_no]\").val();\r\n      var catdesc=$(\"textarea[name=cat_desc]\").val();\r\n      \r\n      var sendData = {};\r\n      var jsonData = {};\r\n      sendData.url =\"index.php/baseset/Crop/add\";\r\n      jsonData.cat_name = catname;\r\n      jsonData.cat_no = bianhao;\r\n      jsonData.cat_chid_name = pinzhong;\r\n      jsonData.ft_type = guoxing;\r\n      jsonData.ft_color = guose;\r\n      jsonData.cat_desc = catdesc;\r\n      sendData.data = jsonData;\r\n      var re = getFaceInfo(sendData);\r\n      if(re.status==1){\r\n            layer.msg(re.msg,{time:1500},function(){\r\n             window.location.reload(); \r\n            });\r\n      }else{\r\n            layer.msg(re.msg);\r\n      }\r\n      /*\r\n      sendData.url =\"27.221.53.90:880/index.php/baseset/Crop/add\";\r\n      jsonData.token = \"3f1tabe0jtumhnr56qkolh44m0\";\r\n      jsonData.phone = \"18114158894\";\r\n      jsonData.cat_name = catname;\r\n      jsonData.cat_no = bianhao;\r\n      jsonData.cat_chid_name = pinzhong;\r\n      jsonData.ft_type = guoxing;\r\n      jsonData.ft_color = guose;\r\n      jsonData.cat_desc = catdesc;\r\n\r\n      sendData.data = jsonData;\r\n      $.ajax({\r\n        url: \"http://www.pc200.com/router.php\",\r\n        data: sendData,\r\n        dataType: \"Json\",\r\n        success: function(re) {\r\n          //alert(msg.data);\r\n          //console.log();\r\n          if(re.status==1){\r\n            layer.msg(re.msg,{time:1500},function(){\r\n             window.location.reload(); \r\n            });\r\n           \r\n          }else{\r\n            layer.msg(re.msg);\r\n          }\r\n          \r\n        }.bind(this),\r\n       \r\n      });*/\r\n\r\n    },\r\n    \r\n    \r\n  },\r\n}\r\n</script>\r\n \r\n\t\r\n<style>\r\n.main-left-m-top {\r\n  width: 100%;\r\n  height: 70px;\r\n  border-bottom: 1px solid #d0dadc;\r\n}\r\n.main-left-m-top-left {\r\n  width: 60%;\r\n  height: auto;\r\n  float: left;\r\n  display: inline;\r\n}\r\n.main-left-m-top-right {\r\n  width: 40%;\r\n  height: auto;\r\n  float: right;\r\n  display: inline;\r\n}\r\n.main-left-m-top-right span{\r\n  padding: 6px 24px;\r\n  background-color: #f2a553;\r\n  border-radius: 3px;\r\n  color: #fff;\r\n  font-size: 14px;\r\n  font-weight: bold;\r\n  float: right;\r\n  display: inline;\r\n  margin-top: 8px;\r\n  margin-left: 18px;\r\n}\r\n.main-left-m-top-right a:visited {\r\n  color: #fff;\r\n  text-decoration: none;\r\n}\r\n.main-left-m-top-right a:hover {\r\n  color: #fff;\r\n  text-decoration: none;\r\n}\r\n.main-left-m-center {\r\n  width: 100%;\r\n  height: 100%;\r\n  margin: 30px 0;\r\n  background-color: #fff;\r\n  padding: 10px 0;\r\n  box-shadow: 0 0 2px #ddd;\r\n}\r\n.main-left-m-center table {\r\n  width: 100%;\r\n  height: auto;\r\n}\r\n.main-left-m-center table tr th {\r\n  padding: 12px 26px;\r\n  font-weight: bold;\r\n  color: #333;\r\n  border-bottom: 1px solid #ddd;\r\n  font-size: 14px;\r\n}\r\n.main-left-m-center table tr td {\r\n  padding: 12px 26px;\r\n  font-weight: normal;\r\n  color: #333;\r\n  font-size: 13px;\r\n}\r\n.new-css-ad {\r\n  border-right: 1px solid #eeeeee;\r\n  vertical-align: middle !important;\r\n}\r\ntable tr th,\r\ntd {\r\n  text-align: center;\r\n}\r\n/**add form**/\r\n.add-form-div {\r\n  width: 94.2%;\r\n  padding: 1% 2.9%;\r\n  background-color: #fff;\r\n  height: 98%;\r\n  display: block;\r\n}\r\n.h4span {\r\n  font-weight: normal;\r\n  font-size: 14px;\r\n}\r\n.control-group {\r\n  display: flex;\r\n  padding: 30px 0;\r\n}\r\n.control-group input {\r\n  margin-left: 12px;\r\n}\r\n.control-group textarea {\r\n  margin-left: 12px;\r\n}\r\n</style>\n\n\n// WEBPACK FOOTER //\n// src/view/baseset/cropmode/grow_add.vue","<template>\r\n<div id=\"left-box\" >\r\n    <div style=\" width: 100%; height: 100%;\" >\r\n        <div class=\"newdivtop\"> \r\n        <h4 style=\"font-weight:bold;\">种植任务&nbsp;&nbsp;<span class=\"h4spana\">|&nbsp;&nbsp;添加种植任务</span>\r\n            <span class=\"h4span-r\">返 回</span>\r\n            <span class=\"h4span-r\" style=\"background-color:#b0c777;\" v-on:click=\"dofabu()\">发 布</span>\r\n        </h4>\r\n        </div> \r\n\r\n        <div class=\"maingropplantop\" >\r\n            <h4 style=\"color:#f2a553;\">生产计划1</h4>\r\n            <div class=\"maingropinfo\">\r\n                <div class=\"maingropinfoleft\">\r\n                    <div class=\"maingropinfolefttop\">\r\n                        <p>作物基本信息</p>\r\n                        <p style=\"font-weight:normal; color:#666;\">作物：<span>{{growadd.info.parent_name}}</span> &nbsp;&nbsp;&nbsp;&nbsp;果型：<span>{{growadd.info.cat_type}}</span> &nbsp;&nbsp;&nbsp;&nbsp;果色：<span>{{growadd.info.cat_color}}</span> &nbsp;&nbsp;&nbsp;&nbsp;品种：<span>{{growadd.info.cat_name}}</span></p>\r\n                        <p style=\"font-weight:normal; color:#666;\">描述：<span>{{growadd.info.cat_desc.substring(0,24)}}...</span></p>\r\n                    </div>\r\n\r\n                        <p>剩余预计产量</p>\r\n                        <!--总产量 - 种植任务 得出-->\r\n                        <p style=\"font-weight:normal;\"><strong class=\"gundongtiao\"><span id=\"per\" class=\"spanwidth\"></span></strong> &nbsp;&nbsp;&nbsp;&nbsp;<span style=\"display: inline-block;\"> Kg</span></p>\r\n\r\n                </div>\r\n\r\n                <div class=\"maingropinforight\">\r\n                    <div class=\"maingropinfolefttop\">\r\n                        <p>时间信息</p>\r\n                        <p style=\"font-weight:normal; color:#666;\">定植时间：<span>{{growadd.info.grow_date}}</span> &nbsp;&nbsp;&nbsp;&nbsp;采收期：<span>{{growadd.info.estimate_get_date_1}} 至 {{growadd.info.estimate_get_date_2}}</span> </p>\r\n                    </div>\r\n\r\n                        <p>剩余种植面积</p>\r\n\r\n                        <p style=\"font-weight:normal;\"> <strong class=\"gundongtiao\"><div id=\"pera\" class=\"spanwidtha\" v-bind:style=\"'width:'+mupx+'px'\"></div></strong> &nbsp;&nbsp;&nbsp;&nbsp;<span style=\"display: inline-block;\">{{mianjilast}} 亩</span> </p>\r\n\r\n                </div>\r\n            </div>\r\n        </div>\r\n        \r\n        <div class=\"maingropplancenter\">\r\n            <h4 style=\"color:#f2a553;\">种植任务信息填写</h4>\r\n            <form class=\"form-inline\">\r\n            <div style=\"width:100%; height:auto;\">    \r\n            <div class=\"\" style=\"width:100%; height:auto;\">\r\n                <table style=\"width: 42%;  height: auto; margin-right:30px; display:inline-block; \">\r\n                    <tr class=\"form-group\">\r\n                        <td><label>种植模式</label></td> \r\n                        <td>\r\n                            <sele